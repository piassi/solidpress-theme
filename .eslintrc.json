{
	"env": {
		"browser": true,
		"es6": true,
		"node": true
	},
	"extends": ["plugin:@wordpress/eslint-plugin/recommended", "prettier"],
	"parser": "@babel/eslint-parser",
	"rules": {
		"prettier/prettier": "error"
	},
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": 2016,
		"sourceType": "module",
		"requireConfigFile": false
	},
	"globals": {
		"wp": true,
		"jQuery": true
	}
}
