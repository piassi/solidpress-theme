{
  "name": "solidpress-theme",
  "version": "1.0.0",
  "description": "This is a WordPress starter theme for developers, it includes:",
  "main": "entry.js",
  "scripts": {
    "build": "webpack --mode=production",
    "dev": "webpack --watch",
    "eslint": "eslint \"src/**/*.{js,jsx}\" --quiet",
    "eslint:fix": "eslint \"src/**/*.{js,jsx}\" --quiet --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,scss}\"",
    "stylelint": "stylelint \"src/**/*.{css,scss}\"",
    "stylelint:fix": "stylelint \"src/**/*.{css,scss}\" --fix",
    "wp-pot": "wp-pot --src '**/*.php' --dest-file 'languages/piassi.pot' --domain 'piassi'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/piassi/solidpress-theme.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/piassi/solidpress-theme/issues"
  },
  "homepage": "https://github.com/piassi/solidpress-theme#readme",
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@babel/preset-env": "^7.20.2",
    "babel-loader": "^9.1.2",
    "browser-sync-webpack-plugin": "^2.3.0",
    "css-loader": "^6.7.3",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "mini-css-extract-plugin": "^2.7.3",
    "sass": "^1.59.3",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.2",
    "webfonts-loader": "^8.0.1",
    "webpack": "^5.76.1",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "bootstrap": "^4.6.2",
    "jquery": "^3.6.4",
    "lozad": "^1.16.0",
    "smoothscroll-polyfill": "^0.4.4"
  }
}
